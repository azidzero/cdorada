RewriteEngine On
Options  FollowSymLinks
#RewriteBase /
#RewriteRule ^index\.php$ - [L]
RewriteCond %{REQUEST_FILENAME} !-f
RewriteCond %{REQUEST_FILENAME} !-d
# LANG + MODULE + SECTION
RewriteCond %{REQUEST_URI} !^/?(cms|crm|crs)/
RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/(.*)$ /cdorada/index.php?lang=$1&m=$2&s=$3&o=$4 [L,NC]
# LANG + MODULE + SECTION
RewriteCond %{REQUEST_URI} !^/?(cms|crm|crs)/
RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z0-9]+)/([a-zA-Z0-9]+)$ /cdorada/index.php?lang=$1&m=$2&s=$3 [L,NC]
# LANG + MODULE
RewriteCond %{REQUEST_URI} !^/?(cms|crm|crs)/
RewriteRule ^([a-zA-Z0-9]+)/([a-zA-Z0-9]+)$ /cdorada/index.php?lang=$1&m=$2 [L,NC]
# LANG
RewriteCond %{REQUEST_URI} !^/?(cms|crm|crs)/
RewriteRule ^([a-zA-Z0-9]+)/$ /cdorada/index.php?lang=$1 [L,NC]